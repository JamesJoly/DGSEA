&lt;/span&gt;&lt;span class=&quot;n&quot;&gt;DGSEA.results.untargeted&lt;/span&gt;&lt;span class=&quot;w&quot;&gt; &lt;/span&gt;&lt;span class=&quot;o&quot;&gt;&amp;lt;-&lt;/span&gt;&lt;span class=&quot;w&quot;&gt; &lt;/span&gt;&lt;span class=&quot;n&quot;&gt;untargeted&lt;/span&gt;&lt;span class=&quot;o&quot;&gt;$&lt;/span&gt;&lt;span class=&quot;n&quot;&gt;DGSEA.Results&lt;/span&gt;&lt;span class=&quot;w&quot;&gt;
